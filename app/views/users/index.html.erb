<div class="page-header">
  <h1>PictureGram Feed</h1>
</div>

<h3>Most Popular Tags</h3>
<% Tag.most_popular.each do |tag| %>
  <li style="list-style-type:circle"><%=link_to "#{tag.name}", tag_path(tag)%></li>
  <br>
  <% end %>

 <br>

<h3>All Users on PictureGram</h3>
<% @users.each do |user| %>
	<li style="list-style-type:circle"><%= link_to "#{user.username.capitalize}", user_path(user) %></li><br>
<% end %>

<h3>See What Others Have Been Saying About Your Pictures!</h3>
<% this_user.received_comments.each do |comment| %>
<%= link_to "#{comment.user.username}" %>: <%= comment.content %><br>
<br>

<% end %>
<br>
<br>
<br>
