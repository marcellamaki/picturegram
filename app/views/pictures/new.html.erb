<%= form_for(@picture) do |picture_form| %>
<%= picture_form.label :title, "Title:" %>
<%= picture_form.text_field :title %><br>
<%= picture_form.label :user_id, "User ID:" %>
<%= picture_form.text_field :user_id %><br>
<%= collection_check_boxes(:picture, :tag_ids, Tag.all, :id, :name) %>
  <%= picture_form.submit %>
<% end %>
